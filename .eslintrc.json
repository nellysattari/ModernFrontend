{
    "env": {
        "browser": true,
        "es6": true,
        "jest":true,
        "node":true,
        "phantomjs":true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
   
    "rules": {
      
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "globals":{
        "document": true,
        "window": true,
        "navigator": true,
        "Challenger": true,
        "Sitecore": true
    }
}